{
  "app_name": "Textile",
  "environments": [
    {
      "arch": "amd64",
      "os": "linux"
    },
    {
      "arch": "amd64",
      "os": "darwin"
    },
    {
      "arch": "amd64",
      "os": "windows",
      "env": {
        "CC": "x86_64-w64-mingw32-gcc",
